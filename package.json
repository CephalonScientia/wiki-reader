{
  "name": "wiki-reader",
  "description": "MediaWiki wiki reader web app",
  "version": "0.0.0",
  "homepage": "https://github.com/CephalonScientia/wiki-reader#readme",
  "private": true,
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": "Scientia <scientia4wiki@gmail.com>",
  "contributors": [],
  "directories": {
    "test": "test"
  },
  "engines": {
    "node": "^17.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "test": "npm run lint && npm run compile && npm run jest",
    "lint": "eslint src/. test/. --config .eslintrc.json --ext .ts --fix",
    "dev": "ts-node-dev --no-notify src/",
    "start": "npm run compile && node lib/",
    "jest": "jest --forceExit",
    "compile": "shx rm -rf lib/ && tsc"
  },
  "standard": {
    "env": [
      "jest"
    ],
    "ignore": []
  },
  "types": "lib/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CephalonScientia/wiki-reader.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CephalonScientia/wiki-reader/issues"
  },
  "dependencies": {
    "@feathersjs/configuration": "^4.5.12",
    "@feathersjs/express": "^4.5.12",
    "@feathersjs/feathers": "^4.5.12",
    "@feathersjs/socketio": "^4.5.13",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "helmet": "^5.0.2",
    "serve-favicon": "^2.5.0",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/serve-favicon": "^2.5.3",
    "eslint": "^8.12.0",
    "jest": "^27.5.1",
    "shx": "^0.3.4"
  }
}
